
@{
    Layout = "~/Views/Shared/EditorSiteLayout.cshtml";
}


@Styles.Render("~/bundles/KeysPageStyles")

<div class="section-banner">

    <div>
        <div class="section-title title-bold title-banner">Display Keys</div>
        <div class="section-title title-small">Manage your secret display key used to connect your <a class="link">order displays</a></div>
    </div>
    
</div>


<div class="menu-section">

    <div id="key-header">
        <div>Keys</div>
        <div>
            <button class="btn btn-outline" id="btn-new-key">@ViewData["keyTableText"]</button>
        </div>
    </div>

    <table id="key-table">
        @Html.Raw(@ViewData["keyTableRows"])
        <a class="loading"></a>
    </table>

</div>

@Scripts.Render("~/bundles/KeysScripts")
